{
  "version": 2,
  "functions": {
    "api/scrape-reviews.js": {
      "maxDuration": 60
    },
    "api/scrape-and-analyze.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}